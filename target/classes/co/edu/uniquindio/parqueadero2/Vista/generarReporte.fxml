<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.parqueadero2.Controlador.GenerarReporteController" spacing="10" alignment="center">
    <padding>
        <Insets top="20" bottom="20" left="20" right="20"/>
    </padding>
    <Button text="Generar Reporte Diario" onAction="#generarReporteDiario"/>
    <Button text="Generar Reporte Mensual" onAction="#generarReporteMensual"/>
    <Label fx:id="reporteLabel" text="Reporte"/>
    <Button text="Volver" onAction="#volver"/>
</VBox>
